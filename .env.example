# Supabase
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
# Use Service Role Key for server-side operations needing elevated privileges
# Make sure RLS is properly configured if using Anon Key for inserts/checks
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Resend
RESEND_API_KEY=YOUR_RESEND_API_KEY
# Email address verified with Resend to send emails FROM
RESEND_FROM_EMAIL=you@yourdomain.com

# Secret for Hashing Emails (make this a strong random string)
EMAIL_HASH_SECRET=YOUR_STRONG_RANDOM_SECRET_STRING_FOR_HASHING